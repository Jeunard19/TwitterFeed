@{
    ViewData["Title"] = "Welcome";

}

<!--Get ScreenName of twitter user-->
<h2>User: @ViewData["Name"]</h2>
<ul>
    <!--Here I create a loop with i starting at 0
    and increment by 1 each turn, i is then added to the end of the string 'Message' inorder
    to retrieve the corresponding ViewData value-->
    @for (int i = 0; i < (int)ViewData["NumTimes"]; i++)
    {

            @if (ViewData[string.Format("Message{0}", i)]!=null){
               <li>   @ViewData[string.Format("Message{0}", i)]</li>
            } else{
                break;
            }


        }
</ul>
